generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id          Int      @id @default(autoincrement())
  url         String   @unique
  shortUrl    String   @unique
  accessCount Int      @default(0)
  createdAt   DateTime @default(now())
}